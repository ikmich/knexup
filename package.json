{
  "name": "knexup",
  "version": "0.0.4-alpha",
  "description": "A cli project starter/helper to get up and running with Knex.js projects.",
  "main": "./dist/index.js",
  "type": "module",
  "bin": {
    "knexup": "dist/bin/index.js",
    "knexup-cli": "dist/bin/index.js"
  },
  "scripts": {
    "clean": "rimraf ./dist && rm knexup-config.js && rimraf ./src/knex",
    "build": "rimraf ./dist && tsc",
    "prepublishOnly": "npm run build",
    "installcli": "./install",
    "tscheck": "tsc --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "knex",
    "cli",
    "database"
  ],
  "bugs": {
    "email": "michfx@gmail.com",
    "url": "https://github.com/ikmich/knexup/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ikmich/knexup"
  },
  "maintainers": [
    "Ikenna Agbasimalo <michfx@gmail.com>"
  ],
  "author": "Ikenna Michael <michfx@gmail.com>",
  "license": "MIT",
  "files": [
    "dist/",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "change-case": "^5.1.2",
    "commander": "^11.1.0",
    "fs-extra": "^11.1.1",
    "pluralize": "^8.0.0",
    "rimraf": "^5.0.5",
    "tempy": "^3.1.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.3",
    "@types/pluralize": "^0.0.32",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "tsx": "^4.1.1",
    "typescript": "^5.2.2"
  },
  "prettier": {
    "printWidth": 120,
    "semi": true,
    "trailingComma": "none",
    "singleQuote": true,
    "tabWidth": 2
  }
}
