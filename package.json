{
  "name": "knexup",
  "version": "1.0.0-alpha",
  "description": "A knex.js project starter helper.",
  "exports": "./dist/index.js",
  "type": "module",
  "bin": {
    "knexup": "dist/bin/index.js",
    "knexup-cli": "dist/bin/index.js"
  },
  "scripts": {
    "clean": "rimraf ./dist && rm knexup-config.js && rimraf ./src/db",
    "build": "rimraf ./dist && tsc",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "demo-init": "npm run build && ts-node dist/bin/index.js -t vendor",
    "demo-create-project": "npm run build && ts-node dist/bin/index.js project --path='/Users/i.agbasimalo/projects/code/knexup-projects' -d mysql"
  },
  "keywords": [
    "knex",
    "knexjs",
    "database"
  ],
  "bugs": {
    "email": "michfx@gmail.com",
    "url": "https://github.com/ikmich/cliyargs/issues"
  },
  "author": "Ikenna Michael <michfx@gmail.com>",
  "license": "ISC",
  "files": [
    "dist/",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "change-case": "^5.1.2",
    "commander": "^11.1.0",
    "fs-extra": "^11.1.1",
    "js-beautify": "^1.14.9",
    "knex": "^3.0.1",
    "mysql2": "^3.6.2",
    "pg": "^8.11.3",
    "pluralize": "^8.0.0",
    "rimraf": "^5.0.5",
    "slugify": "^1.6.6",
    "tempy": "^3.1.0"
  },
